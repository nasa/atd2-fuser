        <mapper resource="config/fuser-data-capture/mybatis/mapper.SfdpsExtensionAllMapper.xml" />
        <mapper resource="config/fuser-data-capture/mybatis/mapper.SmesExtensionMapper.xml" />
        <mapper resource="config/fuser-data-capture/mybatis/mapper.SmesExtensionAllMapper.xml" />
