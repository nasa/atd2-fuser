<?xml version="1.0" encoding="UTF-8" ?>

<Configuration>
	<Appenders>
    	<Console name="STDOUT" target="SYSTEM_OUT">
    		<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS zzz} | %-5p | %t | %-25c{1} | %m%n"/>
    	</Console>

    </Appenders> 
    
	<Loggers>
		<RedirectLogger name="com.mosaicatm" level="info" />

		<Root level="warn">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>

</Configuration>
